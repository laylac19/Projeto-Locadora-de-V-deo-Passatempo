<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="202210021919" author="layla">
        <createSequence incrementBy="1" sequenceName="seq_item" startValue="1"/>
        <createTable tableName="item">
            <column name="id_item" type="INT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_item"/>
            </column>
            <column name="num_serie" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="data_aquisicao" type="DATETIME">
                <constraints nullable="false"/>
            </column>
            <column name="tipo_item" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="ativo" type="BOOLEAN"/>
        </createTable>
    </changeSet>
</databaseChangeLog>