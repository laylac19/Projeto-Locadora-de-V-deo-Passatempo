<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="1664892647695-1" author="layla">
        <createTable tableName="titulo_item">
            <column name="id_titulo" type="INT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_titulo_item"/>
            </column>
            <column name="id_item" type="INT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_titulo_item"/>
            </column>
        </createTable>
        <addForeignKeyConstraint baseColumnNames="id_item" baseTableName="titulo_item" constraintName="FK_TITULO_ITEM_ON_ID_ITEM" referencedColumnNames="id" referencedTableName="item"/>
        <addForeignKeyConstraint baseColumnNames="id_titulo" baseTableName="titulo_item" constraintName="FK_TITULO_ITEM_ON_ID_TITULO" referencedColumnNames="id" referencedTableName="titulo"/>
    </changeSet>
</databaseChangeLog>